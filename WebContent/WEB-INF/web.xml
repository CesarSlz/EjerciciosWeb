<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>EjerciciosWeb</display-name>
  <welcome-file-list>
    <welcome-file>edad.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>ResultadoServlet</servlet-name>
  	<servlet-class>com.slz.ejercicios.ResultadoServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ResultadoServlet</servlet-name>
  	<url-pattern>/resultado</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>ResultadoDiaServlet</servlet-name>
  	<servlet-class>com.slz.ejercicios.ResultadoDiaServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>ResultadoDiaServlet</servlet-name>
  	<url-pattern>/resultadoDia</url-pattern>
  </servlet-mapping>
  
</web-app>